                description="quarter returns correct value at quarter boundaries">
        <munit:execution>
            <ee:transform>
                <ee:message>
                    <ee:set-payload><![CDATA[
                        %dw 2.0
                        import modules::DateUtils
                        output application/json
                        ---
                        {
                            march: DateUtils::quarter(|2026-03-31|),
                            april: DateUtils::quarter(|2026-04-01|),
                            june: DateUtils::quarter(|2026-06-30|),
                            july: DateUtils::quarter(|2026-07-01|)
                        }
                    ]]></ee:set-payload>
                </ee:message>
            </ee:transform>
        </munit:execution>
        <munit:validation>
            <munit-tools:assert-that
                expression="#[payload.march]"
                is="#[MunitTools::equalTo(1)]"/>
            <munit-tools:assert-that
                expression="#[payload.april]"
                is="#[MunitTools::equalTo(2)]"/>
            <munit-tools:assert-that
                expression="#[payload.june]"
